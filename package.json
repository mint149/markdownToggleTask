{
    "name": "markdown-toggle-task",
    "displayName": "Markdown Toggle Task",
    "description": "Toggle tasks in Markdown like Obsidian",
    "version": "0.0.1",
    "license": "MIT",
    "publisher": "hatoo",
    "author": {
        "name": "hatoo"
    },
    "engines": {
        "vscode": "^1.63.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/mint149/markdownToggleTask"
    },
    "categories": [
        "Programming Languages", "Formatters", "Other"
    ],
    "activationEvents": [
        "onCommand:markdownToggleTask.toggleTask"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [{
            "command": "markdownToggleTask.toggleTask",
            "title": "Toggle Task",
            "category": "Markdown",
            "shortTitle": "toggleTask"
        }],
        "keybindings": [{
            "command": "markdownToggleTask.toggleTask",
            "key": "Ctrl+enter",
            "mac": "Cmd+enter",
            "when": "editorTextFocus"
        }]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile && npm run lint",
        "lint": "eslint src --ext ts",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/vscode": "^1.63.0",
        "@types/glob": "^7.1.4",
        "@types/mocha": "^9.0.0",
        "@types/node": "14.x",
        "@typescript-eslint/eslint-plugin": "^5.1.0",
        "@typescript-eslint/parser": "^5.1.0",
        "eslint": "^8.1.0",
        "glob": "^7.1.7",
        "mocha": "^9.1.3",
        "typescript": "^4.4.4",
        "@vscode/test-electron": "^1.6.2"
    }
}